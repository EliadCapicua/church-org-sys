{
  "name": "@acme/validators",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./src/index.ts"
    },
    "./*": {
      "types": "./dist/*.d.ts",
      "default": "./src/*.ts"
    }
  },
  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "clean": "git clean -xdf .cache .turbo dist node_modules",
    "dev": "tsc",
    "format-and-lint": "biome check ./src",
    "typecheck": "tsc --noEmit --emitDeclarationOnly false"
  },
  "dependencies": {
    "zod": "catalog:"
  },
  "devDependencies": {
    "@acme/tsconfig": "workspace:*",
    "@biomejs/biome": "^1.9.4",
    "typescript": "catalog:"
  }
}